{
  "name": "@peptalk/research",
  "version": "0.1.0",
  "description": "Research pipeline for PepTalk peptide synthesis",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "peptalk-research": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "cli": "tsx cli/index.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@peptalk/database": "workspace:*",
    "@peptalk/schemas": "workspace:*",
    "express": "^4.18.2",
    "openai": "^4.24.0",
    "puppeteer": "^21.7.0",
    "tsx": "^4.7.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251014.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
